@{
    ViewBag.Title = "Lijst";
}
<div class="row">
    <div class="col-lg-12">
        <h2>Projecten</h2>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped">
            <thead>
                <tr>
                    <td>Titel</td>
                    <td>Aangemaakt door</td>
                    <td>Status</td>
                    <td>Week</td>
                    <td>Groepen</td>
                    <td>Wijzig</td>
                    <td>Verwijder</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Project barometer</td>
                    <td>Jos van Weert</td>
                    <td><span class="label label-success">Bezig</span></td>
                    <td>2</td>
                    <td>@Html.ActionLink(" ", "ProjectGroups", "Admin", new { @class = "glyphicon glyphicon-align-justify"})</td>
                    <td>@Html.ActionLink(" ", "Detail", "Admin", new { @class = "glyphicon glyphicon-edit"})</td>
                    <td>@Html.ActionLink(" ", "Remove", "Project", new { @class = "glyphicon glyphicon-remove", @Href = "#" })</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
